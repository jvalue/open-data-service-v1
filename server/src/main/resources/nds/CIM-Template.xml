<?xml version="1.0" encoding="UTF-8"?>
<m:Weather xmlns:m="http://iec.ch/TC57/Weather#" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://iec.ch/TC57/Weather# Weather.xsd">
	<m:EnvironmentalMonitoringStation>
		<m:mRID>__ID__</m:mRID>
		<m:EnvironmentalAnalog>
			<m:AtmosphericAnalog>
				<m:unitMultiplier>none</m:unitMultiplier>
				<m:unitSymbol>WPerm2</m:unitSymbol>
				<m:kind>irradianceGlobalHorizontal</m:kind>
				<m:AnalogValues>
					<m:AnalogValue>
						<m:timeStamp>__TIMESTAMP__</m:timeStamp>
						<m:value>__SOLAR_RADIATION__</m:value>
					</m:AnalogValue>
				</m:AnalogValues>
			</m:AtmosphericAnalog>
			<m:AtmosphericAnalog>
				<m:unitMultiplier>none</m:unitMultiplier>
				<m:unitSymbol>degC</m:unitSymbol>
				<m:kind>ambientTemperature</m:kind>
				<m:AnalogValues>
					<m:AnalogValue>
						<m:timeStamp>__TIMESTAMP__</m:timeStamp>
						<m:value>__TEMP_IN_C__</m:value>
					</m:AnalogValue>
				</m:AnalogValues>
			</m:AtmosphericAnalog>
		</m:EnvironmentalAnalog>
		<m:Location>
			<m:mainAddress>
				<m:townDetail>
					<m:name>__CITY__</m:name>
				</m:townDetail>
			</m:mainAddress>
		</m:Location>
	</m:EnvironmentalMonitoringStation>
</m:Weather>
